titles <- c(titles, extractedTitle(data.combined[i,"Name"]))
}
data.combined$Title <- as.factor(titles)
library(ggplot2)
ggplot(data.combined[1:891,], aes(x = Title, fill = Survived)) +
geom_bar(binwidth = 0.5) +
facet_wrap(~Pclass) +
ggtitle("Pclass")   +
xlab("Title")       +
ylab("Total Count") +
labs(fill = "Surivived")
install.packages("ggplot2")
require(XML)
# Upper Cape Tech
# data from the MCAS webiste
UCT <- readHTMLTable("http://profiles.doe.mass.edu/mcas/mcascharts2.aspx?linkid=33&orgcode=08790000&fycode=2015&orgtypecode=5&"
, which=1, header = TRUE)
Ucol <- levels(UCT$V5) # used to obtain the levels of a specific column V5 = 2015
Ueng  <- as.numeric(Ucol)[UCT$V5[5]]
Umath <- as.numeric(Ucol)[UCT$V5[13]]
Usci  <- as.numeric(Ucol)[UCT$V5[21]]
uctTotal <- Ueng + Umath + Usci
# Bourne High School
# data from MCAS website
BHS   <- readHTMLTable("http://profiles.doe.mass.edu/mcas/mcascharts2.aspx?linkid=33&orgcode=00360000&fycode=2015&orgtypecode=5&", which=1)
Bcol  <- levels(BHS$V5)
Beng  <- as.numeric(Bcol)[BHS$V5[5]]
Bmath <- as.numeric(Bcol)[BHS$V5[13]]
Bsci  <- as.numeric(Bcol)[BHS$V5[21]]
bhsTotal <- Beng + Bmath + Bsci
EngSlices  <- c(Ueng  , Bmath)
MathSlices <- c(Umath , Bmath)
SciSlices  <- c(Usci  , Bsci)
TotalSlices <- c(uctTotal,bhsTotal)
chartLabels <- c("UCT total", "BHS total")
pie(MathSlices, labels = chartLabels, main = "MCAS (ADV) Math Comparison")     # Math
pie(EngSlices, labels = chartLabels, main = "MCAS (ADV) English Comparison")   # English
pie(SciSlices, labels = chartLabels, main = "MCAS (ADV) Science Comparison")   # Science
pie(TotalSlices , labels = chartLabels , main = "MCAS (ADV) Total Comparison") # Total
# note this data doesn't mean the number of students
# as one student can score advanced in two or more of the three topics
