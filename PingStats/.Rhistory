#get the title from all records
extractedTitle <- function(name) {
name <- as.character(name)
if (length(grep("Miss.", name)) > 0) {
return("Miss.")
} else if (length(grep("Master.", name)) > 0){
return("Master.")
} else if (length(grep("Mrs.",name)) > 0){
return("Mrs.")
} else if (length(grep("Mr.", name)) > 0){
return("Mr.")
} else {
return("Nothing")
}
}
titles <- NULL
for (i in 1:nrow(data.combined)) {
titles <- c(titles, extractedTitle(data.combined[i,"Name"]))
}
